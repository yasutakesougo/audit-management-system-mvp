{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "BackfillMetrics",
  "type": "object",
  "properties": {
    "runId": { "type": "string" },
    "startedAt": { "type": "string", "format": "date-time" },
    "finishedAt": { "type": "string", "format": "date-time" },
    "durationSec": { "type": "number" },
    "total": { "type": "integer" },
    "created": { "type": "integer" },
    "updated": { "type": "integer" },
    "skipped": { "type": "integer" },
    "conflict409": { "type": "integer" },
    "failures": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "entryId": { "type": "string" },
          "status": { "type": "integer" },
          "reason": { "type": "string" }
        },
        "required": ["entryId", "status", "reason"]
      }
    }
  },
  "required": [
    "runId",
    "startedAt",
    "finishedAt",
    "durationSec",
    "total",
    "created",
    "updated",
    "skipped",
    "conflict409",
    "failures"
  ]
}
