        <pnp:ListInstance Title="Audit_Events" Description="監査ログ" TemplateType="100" Url="Lists/Audit_Events">
          <pnp:Fields>
            <pnp:Field Type="DateTime" DisplayName="タイムスタンプ" Required="TRUE" Indexed="TRUE" StaticName="ts" Name="ts" />
            <pnp:Field Type="Text" DisplayName="実行者(UPN)" Required="TRUE" StaticName="actor" Name="actor" />
            <pnp:Field Type="Text" DisplayName="操作" Required="TRUE" StaticName="action" Name="action" />
            <pnp:Field Type="Text" DisplayName="対象エンティティ" Required="TRUE" StaticName="entity" Name="entity" />
            <pnp:Field Type="Text" DisplayName="対象ID" StaticName="entity_id" Name="entity_id" />
            <pnp:Field Type="Text" DisplayName="チャネル" StaticName="channel" Name="channel" />
